{
  "name": "@pepper_fm/swagger-nuxt-ui",
  "type": "module",
  "version": "0.1.4",
  "private": false,
  "packageManager": "bun@1.3.9",
  "exports": {
    ".": {
      "types": "./dist/lib/types.d.ts",
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.cjs"
    },
    "./styles.css": "./dist/lib/index.css"
  },
  "main": "./dist/lib/index.cjs",
  "module": "./dist/lib/index.mjs",
  "types": "./dist/lib/types.d.ts",
  "bin": {
    "swagger-ui-bridge-install": "scripts/cli/install-bridge.mjs"
  },
  "files": [
    "dist/lib",
    "packages/laravel-bridge",
    "scripts"
  ],
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "build:lib": "vite build --config vite.lib.config.ts && vue-tsc -p tsconfig.lib.json --declaration --emitDeclarationOnly",
    "build:viewer": "vite build --logLevel error --config vite.viewer.config.ts",
    "build:bridge-assets": "vite build --logLevel error --config vite.viewer.config.ts && node ./scripts/sync-bridge-viewer-assets.mjs",
    "prepublishOnly": "npm run build:lib && npm run build:bridge-assets",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "bridge:install": "node ./scripts/cli/install-bridge.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxt typecheck"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.49",
    "@nuxt/ui": "^4.4.0",
    "@vueuse/core": "^13.9.0",
    "nuxt": "^4.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@nuxt/eslint": "^1.4.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.35.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  },
  "resolutions": {
    "estree-walker": "2.0.2",
    "unimport": "4.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
